{
	"opts": {
		"encoding": "utf8",
		"destination": "dist/docs",
		"recurse": true,
		"access": "all"
	},
	"plugins": [
		"node_modules/jsdoc/plugins/markdown",
		"dev/tasks/docs/plugins/comment-fixer"
	],
	"source": {
		"include": [
			"dist/esnext"
		],
		"exclude": [
			"dist/esnext/test",
			"dist/esnext/ckeditor5/core/lib"
		],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	}
}
