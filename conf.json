{
	"opts": {
		"encoding": "utf8",
		"destination": "out/jsdoc",
		"recurse": true,
		"access": "all"
	},
	"plugins": [
		"plugins/markdown",
		"dev/jsdoc/plugins/export-es6-class"
	],
	"source": {
		"include": [
			"dist/esnext"
		],
		"exclude": [
			"dist/esnext/test"
		],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	}
}
